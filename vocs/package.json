    "fetch-benchmarks": "bun scripts/fetch-benchmarks.ts"
  },
  "dependencies": {
    "mermaid": "^11.12.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "vocs": "https://pkg.pr.new/wevm/vocs@bf4a7fd",
    "waku": "1.0.0-alpha.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
